model Planning{
   	id String @id @default(auto()) @map("_id") @db.ObjectId
	task String
	startDate DateTime
	endDate DateTime
	startTime DateTime
	endTime DateTime

  clerkUserId String

    workSiteId String @db.ObjectId
    workSite WorkSite @relation(fields: [workSiteId], references: [id], onUpdate: Cascade, onDelete: Cascade)
    @@index([workSiteId])
} 
