model Company{
    id String @id @default(auto()) @map("_id") @db.ObjectId 
    name String
    slug String @unique
    type String
    phone String
    mail String
    capital Float
    rcs String
    siret String
    ape String
    intraCommunityVat String
    road String
    addressNumber String
    postalCode String
    city String
    additionnalAddress String?
    decennialInsuranceName String
    insuranceContractNumber String @unique
    aeraCoveredByInsurance String
}
