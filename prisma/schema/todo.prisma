model ToDo{
    id String @id @default(auto()) @map("_id") @db.ObjectId
    task String 
    createdAt DateTime @default(now())
    description String ?
    isChecked Boolean
    // permet dâ€™avoir un historique lors des audits
    isArchived Boolean
    // One user created the toDo
    authorClerkId String
    assignedToClerkId String?
    @@index([authorClerkId])

}
